{
  "name": "@darkobits/class-decorators",
  "description": "Decorator factories for classes and class methods.",
  "version": "4.1.1",
  "license": "WTFPL",
  "author": "darkobits",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "scripts": {
    "clean": "del dist",
    "check:styles": "tslint --project tsconfig.json --format codeFrame",
    "check:types": "tsc --emitDeclarationOnly && del **/*.spec.d.ts",
    "test": "jest",
    "prebuild": "run-p clean check:*",
    "build": "babel src --extensions='.ts' --ignore='**/*.spec.ts,**/*.d.ts' --out-dir=dist --source-maps=inline",
    "prebump": "run-p test build",
    "bump": "standard-version",
    "bump:beta": "npm run prebump && standard-version --prerelease=beta"
  },
  "peerDependencies": {
    "@babel/plugin-proposal-decorators": "^7.0.0"
  },
  "dependencies": {
    "is-plain-object": "~2.0.4",
    "ow": "~0.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/plugin-proposal-optional-catch-binding": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.10",
    "@types/ramda": "^0.25.42",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "del-cli": "^1.1.0",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.5",
    "ramda": "^0.25.0",
    "standard-version": "^4.4.0",
    "tslint": "^5.11.0",
    "tslint-xo": "^0.10.0",
    "typescript": "^3.1.6"
  }
}
