{
  "name": "@darkobits/class-decorator",
  "description": "Decorator factories for classes and class methods.",
  "version": "4.0.0",
  "license": "WTFPL",
  "contributors": [
    {
      "name": "Joshua Martin",
      "email": "joshua@frontlawn.net"
    }
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "check:styles": "tslint --project tsconfig.json --format codeFrame",
    "check:types": "tsc --emitDeclarationOnly; rimraf **/*.spec.d.ts",
    "test": "jest",
    "prebuild": "run-p clean check:*",
    "build": "babel src --extensions='.ts' --ignore='**/*.spec.ts,**/*.d.ts' --out-dir=dist --source-maps=inline",
    "prebump": "run-p test build",
    "bump": "standard-version",
    "bump:beta": "npm run prebump && standard-version --prerelease=beta",
    "start": "nodemon --exec \"babel-node --extensions='.ts'\" src/index.ts"
  },
  "peerDependencies": {
    "@babel/plugin-proposal-decorators": "^7.0.0"
  },
  "dependencies": {
    "is-plain-object": "~2.0.4",
    "ow": "~0.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/node": "^7.0.0-beta.49",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-stage-3": "^7.0.0-beta.49",
    "@babel/preset-typescript": "^7.0.0-beta.49",
    "@types/jest": "^23.0.0",
    "@types/node": "^10.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "jest": "^23.1.0",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "standard-version": "^4.4.0",
    "tslint": "^5.10.0",
    "tslint-xo": "^0.8.0",
    "typescript": "^2.9.1"
  }
}
