{
  "name": "@darkobits/class-decorators",
  "description": "Decorator factories for classes and class methods.",
  "version": "4.0.5",
  "license": "WTFPL",
  "contributors": [
    {
      "name": "Joshua Martin",
      "email": "joshua@frontlawn.net"
    }
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "scripts": {
    "clean": "del dist",
    "check:styles": "tslint --project tsconfig.json --format codeFrame",
    "check:types": "tsc --emitDeclarationOnly && del **/*.spec.d.ts",
    "test": "jest",
    "prebuild": "run-p clean check:*",
    "build": "babel src --extensions='.ts' --ignore='**/*.spec.ts,**/*.d.ts' --out-dir=dist --source-maps=inline",
    "prebump": "run-p test build",
    "bump": "standard-version",
    "bump:beta": "npm run prebump && standard-version --prerelease=beta",
    "start": "nodemon --exec \"babel-node --extensions='.ts'\" src/index.ts"
  },
  "peerDependencies": {
    "@babel/plugin-proposal-decorators": "^7.0.0"
  },
  "dependencies": {
    "is-plain-object": "~2.0.4",
    "ow": "~0.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/node": "^7.0.0-rc.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.1",
    "@babel/plugin-proposal-decorators": "^7.0.0-rc.1",
    "@babel/plugin-proposal-optional-catch-binding": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/preset-typescript": "^7.0.0-rc.1",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/ramda": "^0.25.36",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "del-cli": "^1.1.0",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "ramda": "^0.25.0",
    "standard-version": "^4.4.0",
    "tslint": "^5.11.0",
    "tslint-xo": "^0.9.0",
    "typescript": "^3.0.1"
  }
}
